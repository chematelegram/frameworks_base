    <!-- Message for updating an existing app -->
    <string name="old_version_number">インストール済みバージョン: <xliff:g id="old_version">%1$s</xliff:g></string>
    <!-- Message for updating an existing system app -->
    <string name="new_version_number">新しいバージョン: <xliff:g id="new_version">%1$s</xliff:g></string>
    <!-- App lock -->
    <string name="unlock_application"><xliff:g id="label" example="Telegram">%1$s</xliff:g>をロック解除</string>
